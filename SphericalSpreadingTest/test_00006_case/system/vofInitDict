FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      vofInitDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

fieldName       alpha.water_glycerol;
algorithm       SMCA;
writeGeometry   off;
invert          off;
writeAllFields  on;
checkVolume     on;
refinementLevel 4;
relError        -1.0;

distCalc
{
    surfaceType     levelSet;
    surface         sphere;
    center          (0.0025 0.0025 0.002672);
    radius          0.001;
    orientation     inward;
    narrowBandWidth 4.0;
    bulkValue       1.0;
}


